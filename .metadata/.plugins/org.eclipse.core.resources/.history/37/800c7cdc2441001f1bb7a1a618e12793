#ifndef INC_OSKERNEL_H_
#define INC_OSKERNEL_H_

#include "KRTOS_cfg.h"
#include <stdint.h>

typedef struct TCB_s
{
	uint32_t* 	stackPt;
	TCB_t* 		next_thread;
	uint8_t		priority;

} TCB_t;

typedef enum osKernelReturn_e
{
	OSKERNEL_FAIL = 0,
	OSKERNEL_PASS =1
} osKernelReturn_t;

osKernelReturn_t osKernel_ThreadCreate();


#endif /* INC_OSKERNEL_H_ */
